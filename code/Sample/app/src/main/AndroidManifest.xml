<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.sample">

    <uses-permission android:name="android.permission.INTERNET" />

    <application
        android:name=".App"
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:supportsRtl="true"
        android:debuggable="true"
        android:theme="@style/AppTheme">

        <activity android:name=".MainActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />
                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>

        <activity android:name=".MediaProjectionDemo">
        </activity>

        <activity
            android:name=".classLoader.getgameapp.GameAppListActivity"
            android:label="@string/title_activity_list_view_activty"
            android:theme="@style/AppTheme.NoActionBar">
        </activity>

        <activity android:name=".web.WebViewActivity"
            android:label="WebView">
        </activity>

        <activity android:name=".lifecycle.ActivityA"
            android:label="ActivityA"
            android:configChanges="orientation">
        </activity>

        <activity android:name=".lifecycle.ActivityB"
            android:label="ActivityB">
        </activity>

        <activity android:name="com.sample.Fragment.MyActivtiy">
        </activity>

        <activity android:name=".classLoader.ClassLoaderActivity">
        </activity>

    </application>

</manifest>
